<?xml version="1.0" encoding="ISO-8859-1" ?>

 <!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 1.1//EN"
       "http://jakarta.apache.org/struts/dtds/tiles-config_1_1.dtd">


<!--
     This is the examples Tiles definition file
-->

<tiles-definitions>

    <definition name=".examplesdefault" path="/WEB-INF/jsp/layouts/exampleslayout.jsp">
        <put name="center" value="false"/>
        <put name="menu"   value="/WEB-INF/jsp/menu.jsp" />
        <put name="submenu"   value="/WEB-INF/jsp/examples/menu.jsp" />
        <put name="body"   value="" />
        <put name="error"   value="/WEB-INF/jsp/errors.jsp" />
    </definition>

    <definition name=".javascript" extends=".examplesdefault">
        <put name="body"   value="/WEB-INF/jsp/examples/javascript.jsp" />
    </definition>    
    
    <definition name=".chart" extends=".examplesdefault">
        <put name="body"   value="/WEB-INF/jsp/examples/chart.jsp" />
