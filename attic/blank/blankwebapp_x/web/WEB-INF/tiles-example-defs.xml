    <definition name=".cachecrontest" extends=".examplesdefault">
        <put name="body"   value="/WEB-INF/jsp/examples/cachecrontest.jsp" />
    </definition>  

    <definition name=".cachegrouptest" extends=".examplesdefault">
        <put name="body"   value="/WEB-INF/jsp/examples/cachegrouptest.jsp" />
    </definition>      
        
    <definition name=".sendmail" extends=".examplesdefault">
        <put name="body"   value="/WEB-INF/jsp/examples/sendmail.jsp" />
    </definition>     
    
    <definition name=".tab" extends=".examplesdefault">
        <put name="body"   value="/WEB-INF/jsp/examples/tab.jsp" />
    </definition>     

    
    <definition name=".wait" extends=".examplesdefault">
        <put name="body"   value="/WEB-INF/jsp/examples/wait.jsp" />
