d jdbc.idbDriver;
o jdbc:idb=db.props;


e DROP TABLE PEOPLE_TABLE;

e CREATE TABLE PEOPLE_TABLE
      (
      	ID             INT NOT NULL PRIMARY KEY,            
         FNAME          VARCHAR(80),
         LNAME          VARCHAR(80),
         DEPARTMENT     VARCHAR(80),
         EMAIL          VARCHAR(80),
         IMG            VARCHAR(80)                               
      );

