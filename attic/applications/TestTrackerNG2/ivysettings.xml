<ivysettings>
  <settings defaultResolver="chain"/>
  <resolvers>
    <chain name="chain">
  
      <!--
			<url name="archiva" m2compatible="true">
			  <artifact pattern="http://dev.ess.ch/archiva/repository/internal/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			</url>      
      -->
			<url name="ralscha" m2compatible="true">
			  <artifact pattern="http://repository.ralscha.ch/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			</url>       
      
			<url name="jboss" m2compatible="true">
			  <artifact pattern="http://repository.jboss.org/maven2/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			</url>         
      
      <ibiblio name="ibiblio"  m2compatible="true" usepoms="false"/>
      
    </chain>
  </resolvers>
</ivysettings>
