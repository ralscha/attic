<?xml version="1.0" encoding="UTF-8"?>

<project name="HomepageAdmin" default="generate.as3">
 
	<target name="generate.as3">
		<gas3 outputdir="${basedir}/flex-src">
			<classpath>
				<pathelement location="${basedir}/target/classes" />					
			</classpath>
		
			<fileset dir="${basedir}/target/classes">
				<include name="ch/ess/hpadmin/entity/*.class" />
				<include name="ch/ess/hpadmin/model/*.class" />
			</fileset>
		</gas3>
	</target>

</project>
