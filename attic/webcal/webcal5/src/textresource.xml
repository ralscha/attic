<?xml version="1.0" encoding="ISO-8859-1" ?>

<resources>
  <resource key="login.passwordmail.body">    
    <description locale="en">Passwordmail Body</description>
    <description locale="de">Passwortmail Text</description>        

    <text locale="en"><![CDATA[Your password is: ${password}
Username: ${username}    

Please change it after the next log in!
]]>    
    </text>  
    <text locale="de"><![CDATA[Ihr Passwort lautet: ${password}
Benutzername: ${username}    
  
Bitte bei der naechsten Anmeldung aendern!
]]>    
    </text>  
       
    <variable name="password">      
      <description locale="en">Password</description>
      <description locale="de">Passwort</description>        
    </variable>   
    
    <variable name="username">
      <description locale="en">Username</description>
      <description locale="de">Beutzername</description>   
    </variable>    
    
  </resource>
  

  <resource key="login.passwordmail.subject">    
    <description locale="en">Passwordmail Subject</description>
    <description locale="de">Passwortmail Subjekt</description>        

    <text locale="en"><![CDATA[Password for Cal Web Application]]></text>  
    <text locale="de"><![CDATA[Passwort fuer Cal Web Applikation]]></text>  
            
  </resource>
  
  
  <!-- START REMINDER MAIL -->
 
  <resource key="event.reminder.body" feature="event">    
    <description locale="en">Event Reminder Body</description>
    <description locale="de">Ereignis Erinnerung Text</description>        

    <text locale="en"><![CDATA[
Subject:     ${subject}
Start:       ${start}
End:         ${end}

Location:    ${location}
Category:    ${category}

Sensitivity: ${sensitivity}
Importance:  ${importance}

Description: 
${description}
]]>    
    </text>  
    <text locale="de"><![CDATA[
Subjekt:     ${subject}
Start:       ${start}
Ende:        ${end}

Ort:         ${location}
Kategorie:   ${category}

Sensibilität:${sensitivity}
Wichtigkeit: ${importance}

Beschreibung:
${description}
]]>    
    </text>  
       
    <variable name="subject">      
      <description locale="en">Event Subject</description>
      <description locale="de">Ereignis Subjekt</description>        
    </variable> 
    <variable name="start">      
      <description locale="en">Starttime</description>
      <description locale="de">Start</description>        
    </variable>  
    <variable name="end">      
      <description locale="en">Endtime</description>
      <description locale="de">Ende</description>        
    </variable>
    <variable name="location">      
      <description locale="en">Location</description>
      <description locale="de">Ort</description>        
    </variable>
    <variable name="category">      
      <description locale="en">Category</description>
      <description locale="de">Kategorie</description>        
    </variable>
    <variable name="description">      
      <description locale="en">Description</description>
      <description locale="de">Beschreibung</description>        
    </variable>
    <variable name="sensitivity">      
      <description locale="en">Sensitivity</description>
      <description locale="de">Sensibilität</description>        
    </variable>  
    <variable name="importance">      
      <description locale="en">Importance</description>
      <description locale="de">Wichtigkeit</description>        
    </variable>      
  </resource>    

  <resource key="event.reminder.subject" feature="event">    
    <description locale="en">Event Reminder Subject</description>
    <description locale="de">Ereignis Erinnerung Subjekt</description>        

    <text locale="en"><![CDATA[Reminder: ${subject}]]></text>  
    <text locale="de"><![CDATA[Erinnerung: ${subject}]]></text>  
         
    <variable name="subject">      
      <description locale="en">Event Subject</description>
      <description locale="de">Ereignis Subjekt</description>        
    </variable>           
            
  </resource>
  
  <!-- END REMINDER MAIL -->  
  
  
  <!-- START TASK REMINDER MAIL -->
 
  <resource key="task.reminder.body" feature="task">    
    <description locale="en">Task Reminder Body</description>
    <description locale="de">Aufgabe Erinnerung Text</description>        

    <text locale="en"><![CDATA[
Subject:     ${subject}
Start:       ${start}
Due:         ${due}

Location:    ${location}
Category:    ${category}

Priority:  ${importance}

Description: 
${description}
]]>    
    </text>  
    <text locale="de"><![CDATA[
Subjekt:     ${subject}
Start:       ${start}
Fällig:      ${due}

Ort:         ${location}
Kategorie:   ${category}

Priorität: ${importance}

Beschreibung:
${description}
]]>    
    </text>  
       
    <variable name="subject">      
      <description locale="en">Event Subject</description>
      <description locale="de">Ereignis Subjekt</description>        
    </variable> 
    <variable name="start">      
      <description locale="en">Starttime</description>
      <description locale="de">Start</description>        
    </variable>  
    <variable name="location">      
      <description locale="en">Location</description>
      <description locale="de">Ort</description>        
    </variable>
    <variable name="category">      
      <description locale="en">Category</description>
      <description locale="de">Kategorie</description>        
    </variable>
    <variable name="description">      
      <description locale="en">Description</description>
      <description locale="de">Beschreibung</description>        
    </variable>
    <variable name="importance">      
      <description locale="en">Importance</description>
      <description locale="de">Wichtigkeit</description>        
    </variable>      
  </resource>    

  <resource key="task.reminder.subject" feature="task">    
    <description locale="en">Task Reminder Subject</description>
    <description locale="de">Aufgabe Erinnerung Subjekt</description>        

    <text locale="en"><![CDATA[Reminder: ${subject}]]></text>  
    <text locale="de"><![CDATA[Erinnerung: ${subject}]]></text>  
         
    <variable name="subject">      
      <description locale="en">Task Subject</description>
      <description locale="de">Aufgabe Subjekt</description>        
    </variable>           
            
  </resource>
  
  <!-- END TASK REMINDER MAIL -->   
  
  
