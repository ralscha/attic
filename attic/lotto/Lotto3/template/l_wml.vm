  <card id="lq" title="CH Lotto Quoten">
    <do type="unknown" label="Menu"><go href="#menu"/></do> 
		<p>
       $date<br/>
       <br/>
       6 Richtige<br/>
       $lanz6 x $lbet6<br/>
			5 Richtige plus Zz<br/>
       $lanz5+ x $lbet5+<br/>
			5 Richtige<br/>
       $lanz5 x $lbet5<br/>
			4 Richtige<br/>
