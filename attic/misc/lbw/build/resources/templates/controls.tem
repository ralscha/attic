** shc    = Scheduler Control
** header = header
** c      = caption
** d      = detail
** ==========================================
*/

.shc .header {
	font-family : ${scheduler.font.header.family};
	font-size : ${scheduler.font.header.size};
	font-weight : ${scheduler.font.header.weight};
	background-color : ${scheduler.color.bg.header};
	color : ${scheduler.color.text.header};
	width : 100%;
}

.shc .header .c {
	font-family : ${scheduler.font.header.family};
