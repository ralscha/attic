/* ==========================================
** HeadlineControl
**
** Acronym's
** ------------------------------------------
** hdr = header
** c   = caption
** d   = detail
** ==========================================
*/

.hdr .c {
	font-family : Arial, Helvetica, Sans-Serif;
	font-size : 11pt;
	font-weight : bold;
	color: #F57E17;
	border-bottom: 1px solid #80ADBA;
	padding-left : 2px;
	padding-bottom : 2px;
}

/* Detail */
.hdr .d {
	font-family : Arial, Helvetica, Sans-Serif;
	font-size : 8pt;
	font-weight : bold;
	color: #80ADBA;
}

/* ==========================================
** HeadlineControl
**
** Acronym's
** ------------------------------------------
** hdr = header
** c   = caption
** d   = detail
** ==========================================
*/

.hdr .c {
	font-family : ${header.font.label.family};
	font-size : ${header.font.label.size};
	font-weight : ${header.font.label.weight};
	color: ${header.color.text.caption};
	border-bottom: 1px solid ${header.color.border};
	padding-left : 2px;
	padding-bottom : 2px;
}

/* Detail */
.hdr .d {
	font-family : ${header.font.label.family};
	font-size : ${header.font.label.size - 3};
	font-weight : ${header.font.label.weight};
	color: ${header.color.text.detail};
}

/* ==========================================
** ListControl (with frame)
**
** Acronym's
** ------------------------------------------
** lc    = list control
** lcb   = list control body
** lch   = list control header
** even  = even rows
** evens = even rows selected
** odd   = odd rows
** odds  = odd rows selected
** pg    = page thumb
** pgsep = page thumb seperator
** d     = detail
** c     = caption
** cl    = cell
** ==========================================
*/

/* list control body */
.lc .lcb {
	font-family : ${list.font.body.family};
	font-size : ${list.font.body.size};
	font-weight : ${list.font.body.weight};
	border-left: 1px solid ${list.color.border.body};
	border-right: 1px solid ${list.color.border.body};
	border-bottom: 1px solid ${list.color.border.body};
	background-color : ${list.color.bg.body};
	padding: 5px;
}

.lc .lcb INPUT, .lc .lcb SELECT, .lc .lcb TEXTAREA {
	font-family : ${list.font.body.family};
	font-size : ${list.font.body.size};
	font-weight : ${list.font.body.weight};
	color: black;
	background-color : ${list.color.bg.INPUT};
}

.lc .lcb .if {
	font-size : ${list.font.body.size};
	border: 1px solid ${list.color.border.if};
	background-color : ${list.color.bg.if};
	margin-top: 5px;
	margin-bottom: 5px;
	padding: 5px;
}

.lc .lcb .ifnb {
	font-size : ${list.font.body.size};
	margin-top: 5px;
	margin-bottom: 5px;
}

/* header (level 0) */
.lc .lcb .header, .lc .lcb .header TD {
	font-family : ${list.font.header.family};
	font-size : ${list.font.header.size};
	font-weight : ${list.font.header.weight};
	background-color : ${list.color.bg.colheader};
	color : ${list.color.text.colheader};
	padding-left : 5px;
	padding-right : 5px;
	height : 20px;
	vertical-align : middle;
}

.lc .lcb .header TD A {
	color : ${list.color.text.colheader};
}

.lc .lcb .header SPAN {
	font-family : ${list.font.header.family};
	font-size : ${list.font.header.size};
	font-weight : ${list.font.header.weight};
	color : ${list.color.text.colheader};
	vertical-align : middle;
}

/* header1 (level 1) */
.lc .lcb .header1, .lc .lcb .header1 TD {
	font-family : ${list.font.header.family};
	font-size : ${list.font.header.size};
	font-weight : ${list.font.header.weight};
	background-color : ${list.color.bg.colheader1};
	color : ${list.color.text.colheader1};
	padding-left : 5px;
	padding-right : 5px;
	height : 18px;
	vertical-align : middle;
