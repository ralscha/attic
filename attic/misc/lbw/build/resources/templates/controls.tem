	margin-bottom: 2px;
}

/* Caption */
.tlc_nf .tlch .c {
	font-family : ${list.font.header.family};
	font-size : ${list.font.header.size};
	font-weight : ${list.font.header.weight};
	background-color : ${list.color.bg.header};
	color: ${list.color.text.caption};
}

/* Detail */
.tlc_nf .tlch .d {
	font-family : ${list.font.header.family};
	font-size : ${list.font.header.size - 1};
	font-weight : normal;
	background-color : ${list.color.bg.header};
	color: ${list.color.text.detail};
}

/* Page thumb */
.tlc_nf .tlch .pg {
	font-family : ${list.font.header.family};
	font-size : ${list.font.header.size};
	font-weight : ${list.font.header.weight};
	color : black;
	text-decoration : underline;
	cursor: pointer;
	cursor: hand;		/* compatibility with IE < 6.0 */
}

/* Page seperator */
.tlc_nf .tlch .pgsep {
	font-family : ${list.font.header.family};
	font-size : ${list.font.header.size};
	font-weight : ${list.font.header.weight};
	color : black;
}

/* Page thumb */
.tlc_nf .tlch .pg:hover {
}

/* Page thumb selected */
.tlc_nf .tlch .pgs {
	font-family : ${list.font.header.family};
	font-size : ${list.font.header.size};
	font-weight : ${list.font.header.weight};
	color : ${selection.color.bg};
	cursor: pointer;
	cursor: hand;		/* compatibility with IE < 6.0 */
	text-decoration : underline;
}

/* ==========================================
** TabsetControl
**
** Acronym's
** ------------------------------------------
** tsc           = TabSet Control
** sel           = selected
** unsel         = not selected
** body          = body
** disabled      = disabled
** tabScrollBtnL = scroll button left
** tabScrollBtnR = scroll button right
** tabDetail     = detail
** ==========================================
*/

.tsc  {
	font-family : ${tabset.font.label.family};
	font-size : ${tabset.font.label.size};
	font-weight : ${tabset.font.label.weight};
	color: ${tabset.color.text};
}

.tsc .sel, .tsc .sel A {
	color: ${tabset.color.text.sel};
	cursor : pointer;
	cursor: hand;		/* compatibility with IE < 6.0 */
	vertical-align : middle;
}

.tsc .body {
	background-color : ${tabset.color.bg.level1};
	border-left : 1px solid ${tabset.color.border};
	border-right : 1px solid ${tabset.color.border};
	border-bottom : 1px solid ${tabset.color.border};
}

.tsc .unsel, .tsc .unsel A {
	color: ${tabset.color.text.unsel};
	cursor : pointer;
	cursor: hand;		/* compatibility with IE < 6.0 */
	vertical-align : middle;
}

.tsc .disabled {
	color: ${tabset.color.bg.disabled};
	vertical-align : middle;
}

.tsc .tabScrollBtnL {
	padding-left: 3px;
	padding-top: 6px;
	padding-right: 1px;
	vertical-align : baseline;
}

.tsc .tabScrollBtnR {
	padding-top: 6px;
	padding-left: 0px;
	padding-right: 3px;
	vertical-align : baseline;
}

.tsc .tabDetail{
	font-weight: normal;
	padding-left: 20px;
}

/* Hyperlinks */
.tsc a {
	font-family : ${tabset.font.label.family};
	font-size : ${tabset.font.label.size};
	text-decoration : none;
	color : blue;
}


/* ==========================================
** TreeControl
**
** Acronym's
** ------------------------------------------
** tc     = TreeControl
** tlodd  = odd row
** tleven = even row
** ti     = tree item
** tis    = selected tree item
** tol    = outline
** ==========================================
*/

/* TreeControl */
.tc {
	font-family : ${tree.font.label.family};
	font-size : ${tree.font.label.size};
	font-weight : ${tree.font.label.weight};
	border : none;
}

.tc TD {
	border : none;
}

/* odd rows */
.tc .tlodd TR {
	font-family : ${tree.font.label.family};
	font-size : ${tree.font.label.size};
	font-weight : ${tree.font.label.weight};
	background-color : ${tree.color.bg.odd};
}

.tc .tlodd TD {
	border-bottom : 1px solid ${tree.color.border};
}

/* even rows */
.tc .tleven TR {
	font-family : ${tree.font.label.family};
	font-size : ${tree.font.label.size};
	font-weight : ${tree.font.label.weight};
	background-color : ${tree.color.bg.even};
	border : none;
}

.tc .tleven TD {
	border-bottom : 1px solid  ${tree.color.border};
}


/* Tree Control Item */
.ti {
	width : 100%;
	padding-left : 4px;
}

.ti A {
	color : ${tree.color.text.A};
	padding-left : 4px;
	text-decoration : none;
}

/* Selected Tree Control Item */
.tis {
	width : 100%;
	padding-left : 4px;
	background-color : ${tree.color.bg.sel};
}

.tis A {
	font-weight : ${tree.font.label.weight};
	color : ${tree.color.text.A.sel};
	padding-left : 4px;
	text-decoration : none;
}

/* Tree Outline */
.tol TD  {
	padding-left : 0;
}

/* ==========================================
** Syle Textbutton
**
** Acronym's
** ------------------------------------------
** tbtn = Textbutton
** ==========================================
*/

.tbtn {
	font-family : ${button.font.label.family};
	font-size : ${button.font.label.size};
	font-weight: ${button.font.label.weight};
	cursor: pointer;
	cursor: hand;		/* compatibility with IE < 6.0 */
}

/* ==========================================
** Syle Swap Select
**
** Acronym's
** ------------------------------------------
** sws = SwapSelect
** ==========================================
*/

.sws {
	font-family : ${swap.font.label.family};
}

.sws select.left {
	width : 100%;
}

.sws select.right {
	width : 100%;
}

.sws .label_left {
	font-size : ${swap.font.label.size};
	font-weight : ${swap.font.label.weight};
	padding-left: 3px;
	height: 12px;
}

.sws .label_right {
	font-size : ${swap.font.label.size};
	font-weight : ${swap.font.label.weight};
	padding-left: 3px;
	height: 12px;
}

.sws .swsbtnh {
	width: 40px;
}

.sws .swsbtnv {
	height: 40px;
}

.sws .swsbtnh TD {
	padding: 7px;
}

.sws .swsbtnv TD {
	padding-left: 7px;
	padding-right: 7px;
}

/* ==========================================
** StyleSheet for the gauge control
**
** Acronym's
** ------------------------------------------
** gauge  : Gauge Control
** ==========================================
*/
.gauge {
	font-family: ${gauge.font.label.family};
	font-size: ${gauge.font.label.size};
}

/* ==========================================
** Syles for setting formElement="true"
**
** Acronym's
** ------------------------------------------
** feact  = act as formelement
** lch    = ListControl Header
** lcb    = ListControl Body
** tlch   = TreeLisControl Header
** tlcb   = TreeLisControl Body
** tc     = TreeControl
** ti     = tree item
** tis    = tree item selected
** tsc    = TabSetControl
** ==========================================
*/

/* List - links header */
.lch .feact {
	cursor: pointer;
	cursor: hand;		/* compatibility with IE < 6.0 */
}

/* List - links body */
.lcb .feact {
	cursor: pointer;
	cursor: hand;		/* compatibility with IE < 6.0 */
	color: blue;
}

/* TreeList - links header */
.tlch .feact {
	cursor: pointer;
	cursor: hand;		/* compatibility with IE < 6.0 */
}

/* TreeList - links body */
.tlcb .feact {
	cursor: pointer;
	cursor: hand;		/* compatibility with IE < 6.0 */
	color: blue;
}

/* Tree - links */
.tc .feact {
	cursor: pointer;
	cursor: hand;		/* compatibility with IE < 6.0 */
}

/* Tree - label */
.tc .ti .feact, .tc .tis .feact {
	padding-left: 3px;
	cursor: pointer;
	cursor: hand;		/* compatibility with IE < 6.0 */
}

/* TabSet */
.tsc .feact {
	cursor: pointer;
	cursor: hand;		/* compatibility with IE < 6.0 */
}


/* ==========================================
** Popup Window
**
** Acronym's
** ------------------------------------------
**  popup - Popup Window
**
** ==========================================
*/

.popup {
	font-family: Arial, Helvetica, sans-serif;
	font-size : 8pt;
	position : absolute;
	left : 0;
	top : 0;
	border : 1pt solid black;
	padding : 1px;
	z-index : 0;
	color : black;
	background-color : rgb(255,255,231);
}

.popup .header {
	background-color : gray;
	color: white;
	font-weight: bold;
}

.popup A {
	font-family: Arial, Helvetica, sans-serif;
	font-size : 8pt;
	font-weight: normal;
	color: black;
	text-decoration: none;
}